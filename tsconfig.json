{
    "compilerOptions": {
        /* 基础配置 */
        "target": "esnext",
        "module": "ESNext",
        "removeComments": true,
        "sourceMap": true,
        /* 强类型检查配置 */
        "strict": true,
        "noImplicitAny": false,
        /* 模块分析配置 */
        "baseUrl": "./",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "declaration": false,
    },
}